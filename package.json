  "scripts": {
    "transpile": "babel src -d build --copy-files --extensions .ts",
    "clean": "rimraf build",
    "build": "yarn clean && yarn transpile",
    "start": "node build",
    "dev": "nodemon --exec babel-node src --extensions .ts",
    "test": "jest --runInBand",
    "prepare": "husky install",
    "generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:migrate:reset": "prisma migrate reset --force",
    "docker:build": "docker build -t userbase/api . --target dev",
    "docker:start": "docker compose up",
    "docker:down": "docker compose down",
    "docker:clean": "docker system prune -f",
    "docker:stop": "yarn docker:down && yarn docker:clean",
    "sh": "docker exec -it api sh",
    "codegen": "graphql-codegen",
    "init:dev": "babel-node src/cli --extensions .ts",
    "init:app": "babel-node build/cli",
    "lint": "eslint --fix --ext .ts"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "**/*.ts": "yarn lint"
  },
