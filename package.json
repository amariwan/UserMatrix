{
  "name": "user-matrix",
  "version": "1.0.1",
  "description": "user managemnet api server template",
  "main": "build/index.js",
  "repository": "git@github.com:amariwan/UserMatrix.git",
  "author": "amariwan",
  "license": "MIT",
  "type": "module",
  "private": true,
  "scripts": {
    "clean": "rm -rf build",
    "build": "pnpm clean && pnpm transpile",
    "start": "node build",
    "dev": "nodemon --exec  src --extensions .ts",
    "prepare": "husky install",
    "generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:migrate:reset": "prisma migrate reset --force",
    "docker:build": "docker build -t userbase/api . --target dev",
    "docker:start": "docker compose up",
    "docker:down": "docker compose down",
    "docker:clean": "docker system prune -f",
    "docker:stop": "pnpm docker:down && pnpm docker:clean",
    "sh": "docker exec -it api sh",
    "codegen": "graphql-codegen",
    "init:dev": "node src/cli --extensions .ts",
    "init:app": "node build/cli",
    "lint": "eslint . --ext",
    "lint:fix": "eslint . --fix --format codeframe",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "all:fix": "pnpm lint:fix && pnpm prettier:fix",
    "test": "pnpm test:all",
    "test:unit": "jest",
    "test:unit:watch": "jest --watch",
    "test:unit:coverage": "jest --coverage",
    "test:unit:debug": "jest --runInBand --detectOpenHandles --forceExit",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:report": "playwright show-report",
    "test:report:ui": "playwright show-report --ui",
    "test:all": "pnpm test:unit && pnpm test:e2e"
  },
  "dependencies": {
    "@apollo/server": "^5.0.0",
    "@aws-sdk/client-dynamodb": "^3.848.0",
    "@aws-sdk/client-s3": "^3.848.0",
    "@aws-sdk/client-ses": "^3.848.0",
    "@aws-sdk/client-sns": "^3.848.0",
    "@aws-sdk/credential-provider-node": "^3.848.0",
    "@aws-sdk/lib-dynamodb": "^3.848.0",
    "@aws-sdk/s3-request-presigner": "^3.848.0",
    "@graphql-tools/load-files": "^7.0.1",
    "@graphql-tools/merge": "^9.1.0",
    "@graphql-tools/schema": "^10.0.24",
    "@graphql-tools/utils": "^10.9.0",
    "@inquirer/password": "^4.0.16",
    "@inquirer/prompts": "^7.6.0",
    "@prisma/client": "^6.12.0",
    "@sentry/node": "^9.40.0",
    "@sentry/profiling-node": "^9.40.0",
    "@types/request-ip": "^0.0.41",
    "awesome-phonenumber": "^7.5.0",
    "bcryptjs": "^3.0.2",
    "body-parser": "^2.2.0",
    "consola": "^3.4.2",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "dotenv": "^17.2.0",
    "email-templates": "^12.0.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "express-useragent": "^1.0.15",
    "google-auth-library": "^10.1.0",
    "graphql": "^16.11.0",
    "graphql-redis-subscriptions": "^2.7.0",
    "graphql-scalars": "^1.24.2",
    "graphql-subscriptions": "^3.0.0",
    "i18next": "^25.3.2",
    "i18next-fs-backend": "^2.6.0",
    "i18next-http-middleware": "^3.7.4",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "2.0.2",
    "multer-s3": "^3.0.1",
    "nanoid": "^5.1.5",
    "nodemailer": "^7.0.5",
    "numeral": "^2.0.6",
    "pino": "^9.7.0",
    "pino-http": "^10.5.0",
    "prisma": "^6.12.0",
    "pug": "^3.0.3",
    "rate-limit-redis": "^4.2.1",
    "request-ip": "^3.3.0",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.7",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-resolvers": "^4.5.1",
    "@playwright/test": "^1.54.1",
    "@types/bcryptjs": "^3.0.0",
    "@types/cors": "^2.8.19",
    "@types/email-templates": "^10.0.4",
    "@types/express": "^5.0.3",
    "@types/express-useragent": "^1.0.5",
    "@types/ioredis-mock": "^8.2.6",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/multer-s3": "^3.0.3",
    "@types/node": "^24.0.14",
    "@types/numeral": "^2.0.5",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "dotenv-vault": "^1.27.0",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-formatter-codeframe": "^7.32.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^17.21.0",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "husky": "^9.1.7",
    "ioredis-mock": "^8.9.0",
    "jest": "^30.0.4",
    "lint-staged": "^16.1.2",
    "nodemon": "^3.1.10",
    "playwright": "^1.54.1",
    "prettier": "^3.6.2",
    "prettier-plugin-prisma": "^5.0.0",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "**/*.ts": "pnpm lint"
  }
}
